{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "Ticket",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "OPEN",
        "ASSIGNED",
        "IN_PROGRESS",
        "RESOLVED",
        "CLOSED"
      ],
      "default": "OPEN"
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "LOW",
        "MEDIUM",
        "HIGH"
      ],
      "default": "MEDIUM"
    },
    "assignedTo": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "tickets"
    },
    "ownedBy": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "tags": {
      "type": "customField",
      "customField": "plugin::dynamic-enumeration.dynamic-field"
    },
    "source": {
      "type": "enumeration",
      "enum": [
        "MANUAL",
        "AI"
      ]
    },
    "organisation": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organisation.organisation",
      "inversedBy": "tickets"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "TICKET",
        "INCIDENT",
        "REQUEST",
        "PROBLEM",
        "SUGGESTION",
        "OTHER"
      ],
      "default": "TICKET"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "ticket"
    },
    "notes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::note.note",
      "mappedBy": "ticket"
    },
    "activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity.activity",
      "mappedBy": "ticket"
    }
  }
}
